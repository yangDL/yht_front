<!DOCTYPE html>
<html>

    <head>
        <meta charset="utf-8">
        <title>易号通直播平台管理系统</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <link rel="stylesheet" href="css/bootstrap.css">
        <link rel="stylesheet" href="css/font-awesome.min.css">
        <link rel="stylesheet" href="css/style.css">
        <link rel="stylesheet" href="editor/css/wangEditor.min.css">
        <!--[if lt IE 9]>
        <script src="js/ie/respond.min.js"></script>
        <script src="js/ie/html5.js"></script>
        <![endif]-->
    </head>

    <body>
        <header id="header" class="navbar"></header>
        <nav id="nav" class="nav-primary visible-lg"></nav>
        <!-- / nav -->
        <section id="content">
            <section class="main padder">
                <div class="clearfix">
                    <h4><i class="icon-group"></i>管理员模块</h4>
                </div>
                <div class="row">              
                    <div class="col-lg-12">
                        <section class="panel">
                            <header class="panel-heading">
                                <i class="icon-linux"></i>管理员管理
                            </header>
                            <div class="row text-small">
                                <div class="col-lg-1 m-b-mini">
                                    <select id="member-group" class="form-control"></select>
                                </div>
                                <div class="col-lg-1 m-b-mini">
                                    <select id="member-agent" class="form-control">
                                        <option value="0">请选择</option>
                                    </select>
                                </div>
                                <div class="col-lg-1 m-b-mini">
                                    <select id="member-depart" class="form-control">
                                        <option value="0">请选择</option>
                                    </select>
                                </div>
                                <div class="col-lg-1 m-b-mini">
                                    <select id="member-manager" class="form-control">
                                        <option value="0">请选择</option>
                                    </select>
                                </div>
                                <div class="col-lg-1 m-b-mini">
                                    <input id="member-user" class="form-control" placeholder="用户名"></input>
                                </div>
                                <div class="col-lg-1 m-b-mini">
                                    <a id="member-search" href="#" class="btn btn-info">搜 索</a>
                                </div>
                            </div>
                            <div class="pull-out">
                                <table class="table table-striped m-b-none text-small">
                                    <thead>
                                        <tr>
                                            <th>管理员编号</th>
                                            <th>用户名</th>
                                            <th>昵称</th>
                                            <th>所属分组</th>
                                            <th>所属公司</th>
                                            <th>所属部门</th>
                                            <th>所属团队</th>
                                            <th>推广链接</th>
                                            <th>创建时间</th>
                                            <th>最后登录时间</th>
                                            <th>最后登录IP</th>
                                            <th>编辑公司</th>
                                            <th>会员权限</th>
                                            <th>备注</th>
                                            <th width="100">操作</th> 
                                        </tr>
                                    </thead>
                                    <tbody id="adminer-list"></tbody>
                                </table>
                            </div>
                            <footer class="panel-footer">
                                <div class="row">
                                    <div class="col-lg-4">
                                        <button id="adminer-add" class="btn btn-small btn-white">
                                            <i class="icon-plus text"></i>
                                            <span class="text">新增管理员</span>
                                        </button>
                                    </div>
                                    <div class="col-lg-8 text-right text-center-sm">
                                        <ul class="pagination pagination-small m-t-none m-b-none" id="adminer-page"></ul>
                                    </div>
                                </div>
                            </footer>
                        </section>
                    </div>
 
                </div>
            </section>
        </section>
        <!-- footer -->
        <footer id="footer"></footer>
        <div id="plugin"></div>
        <!-- / footer -->
        <script src="js/jquery.min.js"></script>
        <!-- Bootstrap -->
        <script src="js/bootstrap.js"></script>
        <script type="text/javascript" src="../js/md5.js" ></script>
        <!-- app -->
        <script src="js/app.js"></script>
        <script src="js/app.data.js"></script>
        <!-- file input -->
        <script src="js/file-input/bootstrap.file-input.js"></script>
        <script type="text/javascript" src="editor/js/wangEditor.min.js"></script>
        <script type="text/javascript" src="js/base.js" ></script>
        <script src="js/template_html.js" ></script>
        <script type="text/javascript" src="js/adminer.js?v=0.0.0.1" ></script>
    </body>

</html>
