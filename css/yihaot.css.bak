.share-url span.save-title {background-color: #5cb85c;}
.share-url span.glyphicon {color: #5cb85c;}

.download-url a {border: 1px solid #337ab7;}
.download-url span.save-title {background-color: #337ab7;}
.download-url span.glyphicon {color: #337ab7;}

.teacher-desc a {border: 1px solid #f0ad4e;}
.teacher-desc span.save-title {background-color: #f0ad4e;}
.teacher-desc span.glyphicon {color: #f0ad4e;}

.bg-skin a {border: 1px solid #5bc0de;}
.bg-skin span.save-title {background-color: #5bc0de;}
.bg-skin span.glyphicon {color: #5bc0de;}

.btn-list .dropdown-menu {
    min-width: 100px;
    left: 0px;
    padding: 5px 10px;
    z-index:9999999;
}

.qq-list li a {
    width: 85px;
    height: 30px;
    background: #f5f5f5 url(../img/qq_24px.png) no-repeat left;
    background-size: 20px;
    background-position-x: 5px;
    color: #00A0FF;
    border-radius: 5px;
    text-align: center;
    padding-left: 22px;
    border: 1px solid #00A0FF;
}

.share-url .dropdown-menu img {
    display: block;
    width: 100px;
    height: 100px;
    margin: 5px auto;
    overflow: hidden;
}

#new-bill {
    position: absolute;
    z-index: 9;
    width: 90px;
    right: 40px;
    bottom: 100px;
    filter:alpha(opacity=60);
    -moz-opacity:0.6;
    -khtml-opacity: 0.6;
    opacity: 0.6;
}
#new-bill:hover {
    filter:alpha(opacity=100);
    -moz-opacity:1;
    -khtml-opacity: 1;
    opacity: 1;
}
.sub-lide {
    position: absolute;
    z-index: 10;
    width: 100%;
    height: 0px;
    background: #fff;
    bottom: 50px;
    border-top:1px solid #00a0ff;
}
.bf-form {margin-top: 20px;}
.bf-form .bf-label {text-align: right;}
.bf-form li {
    padding: 5px 10px;
    float: left;
    border: 1px solid #CCCCCC;
    margin:5px 10px 0 0;
}
.banner {
    width: 100%;
    height: 60px;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1001;
    background: rgba(45,45,50, 0.6);
}
.jspDrag {background: #222;}

.connect-qq ,.phone-num{
    width: 80%;
    overflow: hidden;
    margin: 10px auto;
}
.connect-qq li {
    float: left;
    margin: 5px 10px;
}
.phone-num {padding: 0 10px; font-size: 16px; color: red;}

.connect-us {display: none;}

.ad-qq {
    position: absolute;
    bottom: 25px;
    left: 50%;
}
.ad-qq li {
    float: left;
    width: 80px;
    margin:0 10px;
}
.download-desc {
    width: 94%;
    margin: 10px auto;
    overflow: hidden;
}
.download-desc h3 {
    color: #00A0FF;
    font-size: 22px;
    font-weight: bolder;
    border-bottom: 1px solid #00A0FF;
    padding: 0 0 10px;
}
.download-desc h4 {
    color: #333;
    font-size: 16px;
    padding: 5px 0;
}

.ad-close {
    position: absolute;
    top: 10px;
    right: 20px;
    font-size:30px; 
    color:#FFF;
    border: none;
    background: none;
    
}

.user {
    float: right;
    height: 30px;
    overflow: hidden;
    margin-left: 15px;
    border-left: 1px solid #A0A0A0;
}
.login-btn {margin-left: 15px;}
.login-btn button {
    height: 30px;
    padding: 5px 10px;
    margin: 0;
}

.user-img {
    width: 30px;
    height: 30px;
    margin-left: 15px;
    float: left;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius:5px;
    overflow: hidden;
}
.user-img img{width: 100%;}
.user-info {
    float: left;
    overflow: hidden;
    height: 30px;
    margin-left: 10px;
    font-size: 12px;
}
.user-info-name {
    color: #00a0ff;
    height: 15px;
    line-height: 15px;
    overflow: hidden;
}
.user-info-name span {
    float: left;
}
.user-info-name a {
    width: 40px;
    height: 15px;
    float: right;
    text-align: right;
}
.user-op {height: 15px; overflow: hidden;}
.user-op a {height: 15px; line-height: 15px;}
@media screen and (max-width: 1490px) {
    .icon-list li { height:60px; }
    .icon-list li img { width: 40px; height: 40px; }
    .qq-list {display: none;}
    /*.phone-call {display: none;}*/
    .connect-us {display: block;}
}
@media (min-width:1490px) and (max-width: 1555px) {
    /*.phone-call {display: none;}*/
    .connect-us {display: block;}
}
@media screen and (max-width: 1180px) {
    .online-users {display: none; width: 0;}
}

#whisper-win {
    width: 700px; 
    height: 480px; 
    position:fixed; 
    top: 50%; left: 50%; 
    margin-top: -240px; margin-left: -350px; 
    z-index:99; 
    background-color:#fff;
}

#whisper-win .whisper-header-box {
    height:40px;
    background-color:red;
}

#whisper-win .whisper-box {
    width: 480px;
    height: 440px;
    overflow:hidden;
    float:left;
}

#whisper-win .whisper-info-box {
    width:220px;
    height: 440px;
    margin-left:480px;
    overflow:hidden;
    background-color: #000;
}
